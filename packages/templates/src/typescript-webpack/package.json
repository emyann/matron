    "babel-loader": "^8.0.5",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "npm-run-all": "^4.1.5",
    "react-dev-utils": "^7.0.1",
    "ts-node": "^7.0.1",
