{
  "name": "matron",
  "version": "0.12.1",
  "description": "CLI for Rapid Application Development with TypeScript and more",
  "author": "Yann Renaudin <renaudin.yann@gmail.com>",
  "repository": "emyann/matron",
  "homepage": "https://github.com/emyann/matron/tree/master/packages/matron#readme",
  "keywords": [
    "Typescript",
    "Webpack",
    "Parcel",
    "Jest",
    "Karma",
    "Jasmine"
  ],
  "license": "MIT",
  "bin": {
    "matron": "dist/bin/matron.js"
  },
  "files": [
