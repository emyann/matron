    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.0",
    "@types/node": "^10.12.11",
    "@types/yargs": "^12.0.5",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  }
}
