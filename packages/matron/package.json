    "@matron/schematics": "file:../schematics",
    "@octokit/rest": "^16.9.0",
    "@types/app-root-path": "^1.2.4",
    "app-root-path": "^2.1.0",
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.1",
    "cross-spawn": "^6.0.5",
    "yargs": "^12.0.5"
  }
}
