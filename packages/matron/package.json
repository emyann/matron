{
  "name": "matron",
  "version": "0.7.2",
  "description": "CLI for Rapid Appplication Development with TypeScript",
  "author": "Yann Renaudin <renaudin.yann@gmail.com>",
  "keywords": [
    "Typescript",
    "Webpack",
    "Parcel",
    "Jest",
    "Karma",
    "Jasmine"
  ],
  "license": "MIT",
  "bin": {
    "matron": "dist/bin/matron.js"
  },
  "files": [
