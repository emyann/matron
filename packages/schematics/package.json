{
  "name": "@matron/schematics",
  "version": "0.15.0",
  "description": "Matron Schematics actions used by matron cli",
  "keywords": [
    "schematics"
  ],
  "author": "Yann Renaudin <renaudin.yann@gmail.com>",
  "license": "MIT",
  "types": "./dist/typings.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./dist/collection.json",
  "scripts": {
