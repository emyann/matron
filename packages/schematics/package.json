    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "dependencies": {
    "@angular-devkit/core": "^7.3.1",
    "@angular-devkit/schematics": "^7.3.1",
    "chance": "^1.0.18",
    "cross-spawn": "^6.0.5",
    "globby": "^9.0.0",
    "minimatch": "^3.0.4",
    "ora": "^3.0.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "@matron/templates": "file:../templates"
  }
}
