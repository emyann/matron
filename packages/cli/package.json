{
  "name": "matron",
  "version": "0.5.2",
  "description": "CLI fot Typescript and Rapid Appplication Development",
  "author": "Yann Renaudin <renaudin.yann@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "bin": {
    "matron": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=developement ts-node ./src/cli.ts",
    "prod": "npm run build && node ./dist/src/cli.js",
    "server": "nodemon ./src/cli.ts",
    "start": "npm run dev",
    "prepublishOnly": "cp -r ./dist/src/* ./dist && rm ./dist/package.json && rm -rf ./dist/src"
  },
  "dependencies": {
    "@matron/schematics": "^0.5.0",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "cross-spawn": "^6.0.5"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.11.2",
    "@types/commander": "^2.12.2",
    "@types/cross-spawn": "^6.0.0",
    "@types/node": "^10.12.11",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  },
  "gitHead": "c0c73baa0bb890adf2a44fe3da4dd4efcfdb797e"
}
